import React from 'react'
import Blog from '../utilities/Blog'
import { useSelector } from 'react-redux'

const Blogs = () => {
  var blogs = useSelector(state => state.anecdotes)

  return (
    <ul style={{ padding: '0'  }}>
      {blogs.map(blog =>
        <Blog key={blog.id} blog={blog} />
      )}
    </ul>
  )
}

export default Blogs